{
  "compilerOptions": {
    "strict": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "ES2021",
    "outDir": ".build",
    "moduleResolution": "node",
    "lib": [
      "ES2021"
    ],
    "rootDir": "./"
  },
  "include": [
    "src/**/*.ts",
    "handler.js"
  ],
}